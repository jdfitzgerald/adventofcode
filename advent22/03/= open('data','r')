import pprint 
pp = pprint.PrettyPrinter(indent=2)

file = open('test','r')
# file = open('data','r')

def priority(c):
    n = ord(c)
    if n > 96: n -= 96
    else: n -= 38
    return n

for line in [l.strip() for l in file]:
    (c1, c2) = (line[0:len(line)-1],line[0:len(line)-1])
    print(line)
    print(c1)
    print(c2)
    break
